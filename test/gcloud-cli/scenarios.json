{
    "gcloud-cli-default": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "gcloud-cli": {
                "version": "latest"
            }
        }
    },
    "gcloud-cli-alpine": {
        "image": "alpine:latest",
        "features": {
            "gcloud-cli": {
                "version": "latest"
            }
        }
    },
    "gcloud-cli-debian-bookworm": {
        "image": "debian:bookworm",
        "features": {
            "gcloud-cli": {
                "version": "latest"
            }
        }
    },
    "gcloud-cli-project-config": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "gcloud-cli": {
                "projectId": "my-test-project",
                "quotaProjectId": "my-quota-project"
            }
        }
    },
    "gcloud-cli-default-arm64": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "platform": "linux/arm64",
        "features": {
            "gcloud-cli": {
                "version": "latest"
            }
        }
    },
    "gcloud-cli-alpine-arm64": {
        "image": "alpine:latest",
        "platform": "linux/arm64",
        "features": {
            "gcloud-cli": {
                "version": "latest"
            }
        }
    },
    "gcloud-cli-debian-bookworm-arm64": {
        "image": "debian:bookworm",
        "platform": "linux/arm64",
        "features": {
            "gcloud-cli": {
                "version": "latest"
            }
        }
    }
}
