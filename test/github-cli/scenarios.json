{
    "github-cli-default": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "github-cli": {
                "version": "latest"
            }
        }
    },
    "github-cli-alpine": {
        "image": "alpine:latest",
        "features": {
            "github-cli": {
                "version": "latest"
            }
        }
    },
    "github-cli-debian": {
        "image": "debian:latest",
        "features": {
            "github-cli": {
                "version": "latest"
            }
        }
    },
    "github-cli-ssh-config": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "github-cli": {
                "authSsh": true
            }
        }
    },
    "github-cli-git-install": {
        "image": "alpine:latest",
        "features": {
            "github-cli": {
                "installGit": true
            }
        }
    },
    "github-cli-git-config": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "github-cli": {
                "configureGit": true
            }
        }
    },
    "github-cli-op-install": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "github-cli": {
                "install1PasswordCli": true
            }
        }
    }
}
