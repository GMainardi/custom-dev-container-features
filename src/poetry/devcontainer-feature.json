{
    "name": "Poetry",
    "id": "poetry",
    "version": "1.0.1",
    "description": "Installs Poetry, a tool for dependency management and packaging in Python.",
    "containerEnv": {},
    "platforms": ["linux/amd64", "linux/arm64"],
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select version of Poetry to install."
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/python"
    ]
}

