{
    "name": "Google Cloud CLI",
    "id": "gcloud-cli",
    "version": "1.0.3",
    "description": "Installs the Google Cloud CLI",
    "containerEnv": {},
    "platforms": ["linux/amd64", "linux/arm64"],
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select version of gcloud cli to install."
        },
        "projectId": {
            "type": "string",
            "proposals": [],
            "default": "",
            "description": "The Google Cloud project ID to set as default."
        },
        "quotaProjectId": {
            "type": "string",
            "proposals": [],
            "default": "",
            "description": "The Google Cloud project ID to set as the quota project."
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
