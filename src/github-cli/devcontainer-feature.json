{
    "name": "GitHub CLI",
    "id": "github-cli",
    "version": "1.0.6",
    "description": "Installs the GitHub CLI",
    "containerEnv": {},
    "platforms": ["linux/amd64", "linux/arm64"],
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select version of GitHub CLI to install."
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
